{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "vite.config.ts"
  ],
  "paths": {
    // 用于设置模块名到基于baseUrl的路径映射
    "@/*": [
      "src/*"
    ],
    "cps/*": [
      "src/components/*"
    ]
  }
}